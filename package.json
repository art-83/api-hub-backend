{
    "name": "api-hub",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node src/index.ts",
        "build": "tsc && tsc-alias",
        "f": "prettier --write .",
        "typeorm": "ts-node ./node_modules/typeorm/cli.js",
        "migration:generate": "npm run typeorm -- -d src/@config/data-source.config.ts migration:generate database/migrations/$npm_config_name",
        "migration:run": "npm run typeorm -- migration:run",
        "migration:revert": "npm run typeorm -- migration:revert",
        "docker:migration:generate": "docker-compose exec server npm run migration:generate",
        "docker:migration:run": "docker-compose exec server npm run migration:run",
        "docker:migration:revert": "docker-compose exec server npm run migration:revert"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "@types/cors": "^2.8.19",
        "celebrate": "^15.0.3",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "pg": "^8.16.3",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.27"
    },
    "devDependencies": {
        "@types/express": "^5.0.3",
        "@types/node": "^24.8.1",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.9.3"
    }
}
